  "devDependencies": {
    "@astrojs/check": "^0.9.0",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.23",
    "prettier": "^3.3.0",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "typescript": "^5.5.0"
